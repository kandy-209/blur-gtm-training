{
  "name": "blur-gtm-training",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "setup:prospect-db": "node scripts/setup-prospect-intelligence-db.js",
    "db:migrate:prospect": "bash scripts/run-prospect-intelligence-migration.sh",
    "dev": "next dev",
    "dev:debug": "next dev --inspect",
    "dev:debug-brk": "NODE_OPTIONS=--inspect-brk next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:edge-cases": "jest --testPathPattern='edge-cases' --no-coverage",
    "test:workflows": "jest --testPathPattern='workflows' --no-coverage",
    "test:security": "jest --testPathPattern='security' --no-coverage",
    "test:comprehensive": "jest --testPathPattern='comprehensive' --no-coverage",
    "test:stress": "jest --testPathPattern='stress' --no-coverage",
    "test:chaos": "jest --testPathPattern='chaos' --no-coverage",
    "test:all-workflows": "jest --testPathPattern='workflows|edge-cases|security|stress|chaos' --no-coverage",
    "test:all-enhanced": "jest --testPathPattern='workflows|edge-cases|security|stress|chaos|comprehensive' --no-coverage --verbose",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:windows": "powershell -ExecutionPolicy Bypass -File scripts/run-tests.ps1",
    "test:cache": "powershell -ExecutionPolicy Bypass -File scripts/run-all-cache-tests.ps1",
    "test:debug": "node --inspect-brk node_modules/.bin/jest",
    "test:domain": "jest src/__tests__/domain",
    "test:agents": "jest src/__tests__/infrastructure/agents",
    "test:api": "jest src/__tests__/api",
    "test:verbose": "jest --verbose",
    "test:ui": "jest src/__tests__/components",
    "setup:production": "bash scripts/setup-production.sh",
    "setup:interactive": "node setup-production.js",
    "db:migrate": "echo 'Run scripts/create-supabase-tables.sql in Supabase SQL Editor'",
    "setup:supabase": "bash scripts/setup-supabase.sh",
    "verify:supabase": "node scripts/verify-supabase-setup.js",
    "copy:migration": "bash scripts/copy-migration-sql.sh",
    "deploy": "bash deploy.sh",
    "env:add": "node add-env-interactive.js",
    "test:design-system": "bash scripts/test-design-system.sh",
    "test:performance": "node scripts/test-performance.js",
    "test:lighthouse": "npx lighthouse http://localhost:3000 --view",
    "test:a11y": "npx @axe-core/cli http://localhost:3000 || echo 'Install @axe-core/cli: npm install -g @axe-core/cli'",
    "test:backend": "bash scripts/test-backend.sh",
    "test:integration": "node scripts/test-integration.js",
    "test:prospect-intelligence": "node test-prospect-intelligence.js",
    "test:prospect-intelligence:full": "node test-prospect-intelligence.js --full-test",
    "test:all": "npm run typecheck && npm test && npm run test:backend",
    "sync": "powershell -ExecutionPolicy Bypass -File scripts/sync-git.ps1",
    "sync:mac": "bash scripts/sync-git.sh",
    "git:push": "powershell -ExecutionPolicy Bypass -File scripts/safe-git-push.ps1",
    "git:push:mac": "bash scripts/quick-git-push.sh",
    "check:updates": "powershell -ExecutionPolicy Bypass -File scripts/check-updates.ps1",
    "check:updates:mac": "bash scripts/check-updates.sh",
    "update:deps": "powershell -ExecutionPolicy Bypass -File scripts/auto-update-dependencies.ps1 -DryRun",
    "update:deps:apply": "powershell -ExecutionPolicy Bypass -File scripts/auto-update-dependencies.ps1",
    "update:deps:patch": "powershell -ExecutionPolicy Bypass -File scripts/auto-update-dependencies.ps1 -PatchOnly",
    "modal:deploy": "bash modal_functions/deploy.sh",
    "modal:setup": "bash modal_functions/setup_secrets.sh",
    "test:sales-call": "powershell -ExecutionPolicy Bypass -File scripts/test-sales-call-api.ps1",
    "test:modal": "powershell -ExecutionPolicy Bypass -File scripts/test-modal-integration.ps1",
    "api-keys:check": "powershell -ExecutionPolicy Bypass -File scripts/setup-api-keys.ps1 -CheckOnly",
    "api-keys:setup": "powershell -ExecutionPolicy Bypass -File scripts/setup-api-keys.ps1",
    "api-keys:generate": "powershell -ExecutionPolicy Bypass -File scripts/setup-api-keys.ps1 -GenerateEnvFile",
    "api-keys:auto-setup": "powershell -ExecutionPolicy Bypass -File scripts/auto-setup-all-api-keys.ps1",
    "api-keys:supabase": "powershell -ExecutionPolicy Bypass -File scripts/quick-setup-supabase.ps1",
    "api-keys:create-env": "powershell -ExecutionPolicy Bypass -File scripts/create-env-local.ps1",
    "api-keys:sync-vercel": "powershell -ExecutionPolicy Bypass -File scripts/sync-vercel-env-to-local.ps1",
    "fix:powershell": "powershell -ExecutionPolicy Bypass -File scripts/fix-powershell-execution-policy.ps1"
  },
